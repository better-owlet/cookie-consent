{
  "name": "@anowlet/cookie-consent",
  "version": "1.0.2",
  "description": "",
  "keywords": [],
  "main": "dist/cookie-consent.umd.js",
  "unpkg": "dist/cookie-consent.umd.js",
  "module": "dist/cookie-consent.esm.js",
  "source": "src/index.js",
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "src",
    "types"
  ],
  "author": "Anowlet",
  "repository": {
    "type": "git",
    "url": "https://github.com/anowlet/cookie-consent.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=10.18"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "check-types": "tsc --noEmit",
    "lint": "eslint src --fix --ext .ts,.tsx,.js,.jsx",
    "format": "prettier --write \"{src,test}/**/*.{js,ts}\" \"**/*.md\"",
    "format:check": "prettier --list-different \"{src,test}/**/*.{js,ts}\" \"**/*.md\"",
    "prebuild": "rimraf lib dist es coverage types",
    "build": "cross-env rollup -c rollup.config.ts",
    "start": "cross-env NODE_ENV=development rollup -c rollup.config.ts -w",
    "typedoc": "typedoc --out docs --target es6 --theme minimal --mode file src",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "commit": "git-cz",
    "release": "semantic-release",
    "semantic-release-prepare": "ts-node scripts/semantic-release-prepare",
    "precommit": "lint-staged",
    "prepush": "npm run test:prod && npm run build",
    "commitmsg": "commitlint -E HUSKY_GIT_PARAMS",
    "semantic-release": "semantic-release"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "npm run lint",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^9.0.1",
    "@commitlint/config-conventional": "^8.3.4",
    "@rollup/plugin-commonjs": "^12.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.3.2",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/npm": "^7.0.5",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@types/jest": "^25.2.3",
    "@types/lodash": "^4.14.155",
    "@types/node": "^14.0.10",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "colors": "^1.4.0",
    "commitizen": "^4.1.2",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.2",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "jest-config": "^26.0.1",
    "lint-staged": "^10.2.8",
    "prettier": "^2.0.5",
    "prompt": "^1.0.0",
    "replace-in-file": "^6.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.13.1",
    "rollup-plugin-filesize": "^9.0.0",
    "rollup-plugin-livereload": "^1.3.0",
    "rollup-plugin-postcss": "^3.1.1",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript2": "^0.27.1",
    "semantic-release": "^19.0.3",
    "shelljs": "^0.8.4",
    "ts-jest": "^26.1.0",
    "ts-node": "^8.10.2",
    "typedoc": "^0.17.7",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "lodash": "^4.17.15",
    "redom": "^3.27.1"
  }
}
